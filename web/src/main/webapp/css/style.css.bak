.wrap2{background:url(../images/top_bottom.gif) left no-repeat top;width:950px; margin-left:auto; margin-right:auto;border:#6a838a 1px solid; padding:0px 0px 0px 0px;overflow:hidden; clear:both; }

.index_left{ width:510px; float:left; border-right:#cccccc dashed 1px; }
