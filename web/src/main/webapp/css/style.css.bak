@charset "utf-8";

/* CSS Document */

body,p,input,h1,h2,h3,h4,ul,li,dl,dt,dd,form,textarea{
margin:0;padding:0;list-style:none;vertical-align:middle;
}
body{
color:#555555;text-align:center;font-size:12px;
}
.hr1{color:#E1E1E1;background-color:#E1E1E1;height:1px;line-height:1px;font-size:0;border:none;}
.td_dash{color:#7b7c7c; border-bottom:#c9c7c8 dotted 1px;}
a {color:#7b7c7c;text-decoration:none;}
a:visited {color:#7b7c7c;text-decoration:none;}
a:active {color:#008f89;text-decoration:none;}
a:hover {color:#696969;text-decoration:underline;}

.more a {color:#cc9933;text-decoration:none;}
.more a:visited {color:#cc9933;text-decoration:none;}
.more a:active {color:#cc9933;text-decoration:none;}
.more a:hover {color:#cc9933;text-decoration:underline;}
img{ border:0px;}
.header{ 
background:url(../images/index_icon_10.gif) no-repeat left; height:95px; width:950px; margin-left:auto; margin-right:auto; clear:both;
}
.header_right{ background:url(../images/index_14.jpg) no-repeat right bottom; float:right; clear:left; width:619px; height:95px; text-align:center}
.search{ height:68px;height:80px!important;text-align:right; color:#888888; font-size:12px; padding-top:2px;}
.span_search{ background:url(../images/icon_sear.gif) left no-repeat; padding-left:20px; padding-right:10px;}
.span_search a {text-decoration:none;z-index:1;color:#888888; width:100%;}
.span_search a:visited {color:#888888;}
.span_search a:hover {text-decoration:none;color:#008F89;}
.input_seabox{ border:#c1c1c1 solid 1px; height:16px; width:150px; margin-right:5px;}
.input_normal{ border:#c1c1c1 solid 1px; margin-right:5px;}
